{
    "compilerOptions": {
        "target": "es2017",
        "module": "NodeNext",
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": false,
        "allowUnreachableCode": false,
        "noImplicitAny": false,
        "strictPropertyInitialization": false,
        "moduleResolution": "NodeNext",
    },
    "exclude": ["node_modules", "webpack", "dist"]
  }